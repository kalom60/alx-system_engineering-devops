[Unit]
Description=Gunicorn instance to serve dynamic
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=/home/ubuntu/AirBnB_clone_v4/gunicorn --workers 3 --bind unix:AirBnB_clone_v4.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target
